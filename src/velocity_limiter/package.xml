<?xml version="1.0"?>
<package format="3">
  <name>velocity_limiter</name>
  <version>0.0.1</version>
  <description>Intermediate policy node that clamps linear/angular velocity between avoidance and smoothing nodes.</description>
  <maintainer email="example@example.com">Daniel Aguilar</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_python</buildtool_depend>

  <exec_depend>rclpy</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>launch</exec_depend>
  <exec_depend>launch_ros</exec_depend>
  <exec_depend>ament_index_python</exec_depend>

  <export><build_type>ament_python</build_type></export>
</package>

